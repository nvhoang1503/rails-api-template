default: &default
  adapter: postgresql
  encoding: unicode
  username: postgres_username
  password: postgres_password
  host: localhost
  pool: 5
  timeout: 5000
  port: 5432

development:
  <<: *default
  database: satv_development

test:
  <<: *default
  database: satv_test

production:
  <<: *default
  database: satv_production

staging:
  <<: *default
  database: satv_staging